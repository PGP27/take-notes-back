{
  "version": 2,
  "env": {
    "STRING_CONNECTION": "@mongo_uri",
    "SECRET_KEY": "@secret_key"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PATCH", "DELETE"]
    }
  ]
}
